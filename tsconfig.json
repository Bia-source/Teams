{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ], 
      "@screens/*": [
       "./src/screens/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],"@theme/*": [
        "./src/theme/*"
      ],"@utils/*": [
        "./src/utils/*"
      ],"@shared/*": [
        "./src/shared/*"
      ],"@storage/*": [
        "./src/storage/*"
      ],
    }
  }
}
